[user]
	name = Keir Lawson
	email = keirlawson@gmail.com
[core]
	autocrlf = input
[push]
	default = simple
[init]
	templatedir = ~/.git_templates
[merge]
	tool = meld
[diff]
	compactionHeuristic = true
[alias]
	co = checkout
	br = branch
	fp = push --force-with-lease
[credential]
	helper = gnome-keyring
[pull]
	rebase = true
